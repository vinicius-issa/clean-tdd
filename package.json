    "lint-staged": "9.5.0",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.1",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.5",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
